	<queryString>
		<![CDATA[SELECT
     BaoCaoThongKe."id_baocao" AS BaoCaoThongKe_id_baocao,
     BaoCaoThongKe."loai_bao_cao" AS BaoCaoThongKe_loai_bao_cao,
     BaoCaoThongKe."tongtien" AS BaoCaoThongKe_tongtien,
     BaoCaoThongKe."thong_tin_chi_tiet" AS BaoCaoThongKe_thong_tin_chi_tiet,
     BaoCaoThongKe."ngay_tao_bao_cao" AS BaoCaoThongKe_ngay_tao_bao_cao,
     ChiTietBaoCao."id_chitietbaocao" AS ChiTietBaoCao_id_chitietbaocao,
     ChiTietBaoCao."id_baocao" AS ChiTietBaoCao_id_baocao,
     ChiTietBaoCao."id_hoadon" AS ChiTietBaoCao_id_hoadon,
     ChiTietBaoCao."idban" AS ChiTietBaoCao_idban,
     ChiTietBaoCao."sotien" AS ChiTietBaoCao_sotien,
     DonHang."order_id" AS DonHang_order_id,
     DonHang."id_ban" AS DonHang_id_ban,
     DonHang."gia_giam" AS DonHang_gia_giam,
     DonHang."TongBill" AS DonHang_TongBill,
     DonHang."tongtien" AS DonHang_tongtien,
     DonHang."trang_thai_thanhtoan" AS DonHang_trang_thai_thanhtoan,
     Ban."id_ban" AS Ban_id_ban,
     Ban."ten_ban" AS Ban_ten_ban,
     Ban."ngay_dat" AS Ban_ngay_dat,
     Ban."trang_thai" AS Ban_trang_thai,
     Ban."ghi_chu" AS Ban_ghi_chu
FROM
     "dbo"."BaoCaoThongKe" BaoCaoThongKe INNER JOIN "dbo"."ChiTietBaoCao" ChiTietBaoCao ON BaoCaoThongKe."id_baocao" = ChiTietBaoCao."id_baocao"
     INNER JOIN "dbo"."DonHang" DonHang ON ChiTietBaoCao."id_hoadon" = DonHang."order_id"
     INNER JOIN "dbo"."Ban" Ban ON ChiTietBaoCao."idban" = Ban."id_ban"
     AND Ban."id_ban" = DonHang."id_ban"
WHERE
     MONTH(Ban."ngay_dat") = MONTH($P{ngay_dat})]]>
	</queryString>
